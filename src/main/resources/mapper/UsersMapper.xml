<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zizi.qmusic.server.mapper.UsersMapper">


    <select id="getUserInfo" resultType="com.zizi.qmusic.server.domain.UsersDO">
        select * from q_user
    </select>

    <select id="getDetail" resultType="com.zizi.qmusic.server.domain.UsersDO">
        select * from q_user where username=#{username}
    </select>

</mapper>